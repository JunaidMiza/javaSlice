const hasDriverLicence = true;
const hasGoodVision = false;
const clearCheck = true;
console.log(hasDriverLicence && clearCheck);
console.log(hasDriverLicence || (hasGoodVision && clearCheck));
hasDriverLicence && hasGoodVision && clearCheck
  ? console.log(`you are able to ride `)
  : console.log(`you are not allowed sorry sir`);
