coding challenge 1\
Data 1;

let johnsMass = 92;
let johnsHight = 1.95;
let marksMass = 78;
let marksHight = 1.69;
const bmiJohns = johnsMass / johnsHight ** 2;
const bmiMarks = marksMass / marksHight ** 2;
console.log(bmiJohns, bmiMarks);
let max = bmiJohns > bmiMarks
    ? console.log(`johns has greater BMI then marks`)
    : console.log(`marks has greater BMI then marks`);
Data 2;
let johnsMass = 85;
let johnsHight = 1.76;
let marksMass = 95;
let marksHight = 1.88;
const bmiJohns = johnsMass / johnsHight ** 2;
const bmiMarks = marksMass / marksHight ** 2;
console.log(bmiJohns, bmiMarks);
let max =
  bmiJohns > bmiMarks
    ? console.log(`johns has greater BMI then marks`)
    : console.log(`marks has greater BMI then marks`);
